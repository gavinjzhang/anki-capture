{
  "name": "anki-capture-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --remote",
    "deploy": "wrangler deploy",
    "db:init": "wrangler d1 execute anki-capture --file=./schema.sql",
    "db:init:local": "wrangler d1 execute anki-capture --local --file=./schema.sql",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "jose": "^5.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@cloudflare/vitest-pool-workers": "^0.5.0",
    "@vitest/coverage-v8": "^2.1.8",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8",
    "wrangler": "^4.0.0"
  }
}
